[sub_resource type="Shader" id=1]
code = "shader_type canvas_item;

uniform sampler2D frame1;
uniform sampler2D frame2;
uniform sampler2D frame3;
uniform sampler2D frame4;
uniform sampler2D frame5;

uniform float speed = 1.0;

void fragment()
{
	float time = fract(TIME * speed);
	vec4 col;
	
	if (time < 0.2)
	{
		col = texture(frame1, UV);
	}
	else if (time < 0.4)
	{
		col = texture(frame2, UV);
	}
	else if (time < 0.6)
	{
		col = texture(frame3, UV);
	}
	else if (time < 0.8)
	{
		col = texture(frame4, UV);
	}
	else
	{
		col = texture(frame5, UV);
	}
	
	COLOR = col;
}"

